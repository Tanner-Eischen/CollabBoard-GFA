{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run --coverage",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\""
  },
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.0.5"
  }
}
