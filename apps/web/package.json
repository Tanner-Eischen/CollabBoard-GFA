{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "prisma generate --schema=../server/prisma/schema.prisma && next build",
    "build:vercel": "cd ../.. && pnpm turbo build --filter=web",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run --coverage",
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "db:generate": "prisma generate --schema=../server/prisma/schema.prisma"
  },
  "dependencies": {
    "@collabboard/shared": "workspace:*",
    "@prisma/client": "^6.1.0",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "konva": "^10.2.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-konva": "^18.2.14",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.47",
    "prisma": "^6.1.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vitest": "^3.0.5",
    "vitest-canvas-mock": "^1.1.3"
